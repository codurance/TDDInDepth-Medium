{
    "name": "tdd-in-depth-frontend-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest",
        "test:component": "cypress open --component --browser electron",
        "test:e2e": "cypress open --e2e --browser chrome",
        "test:mutation": "stryker run",
        "dev:server": "npx json-server db.json -p 5000 "
    },
    "dependencies": {
        "@tanstack/react-query": "^5.52.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@stryker-mutator/vitest-runner": "^8.5.0",
        "@testing-library/cypress": "^10.0.2",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "cypress": "^13.14.0",
        "eslint": "^9.9.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "globals": "^15.9.0",
        "jsdom": "^25.0.0",
        "msw": "^2.3.5",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1",
        "vitest": "^2.0.5"
    }
}
